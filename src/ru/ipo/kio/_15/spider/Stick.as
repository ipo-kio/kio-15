package ru.ipo.kio._15.spider {
import flash.display.Sprite;

public class Stick extends Sprite {

    var x1:Number;
    var y1:Number;
    var x2:Number;
    var y2:Number;

    public function Stick(x1:Number, y1:Number, x2:Number, y2:Number) {
        this.x1 = x1;
        this.y1 = y1;
        this.x2 = x2;
        this.y2 = y2;
    }


}
}
